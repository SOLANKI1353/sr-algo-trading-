generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  // Add other fields if your project uses users
}

model ApiKey {
  id        Int      @id @default(autoincrement())
  name      String   @default("angel")
  key       String
  secret    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
